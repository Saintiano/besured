<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.firebaseapp.clovis_saintiano.besecured">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".navigation_activities.Home_Activity" />
        <activity android:name=".Login_Activity" />
        <activity android:name=".SignUp_Activity" />
        <activity android:name=".PassWord_Recovery_Activity" />
        <activity android:name=".navigation_activities.GetHelp_Activity" />
        <activity android:name=".navigation_activities.Map_Activity" />
        <activity android:name=".navigation_activities.Places_Activity" />
        <activity android:name=".navigation_activities.Profile_Activity"></activity>
    </application>

</manifest>